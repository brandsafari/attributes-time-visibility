{
	"name": "@brandsafari/developer-starter",
	"version": "1.0.0",
	"description": "Developer starter template for Brandsafari projects.",
	"homepage": "https://github.com/Brandsafari/developer-starter#readme",
	"license": "ISC",
	"keywords": [],
	"author": {
		"name": "Brandsafari"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brandsafari/developer-starter.git"
	},
	"type": "module",
	"main": "src/index.js",
	"module": "src/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "cross-env NODE_ENV=development node ./bin/build.js",
