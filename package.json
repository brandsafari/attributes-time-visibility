		"release": "changeset publish",
		"update": "npm update -i -L -r"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"esbuild": "^0.17.7",
		"prettier": "^2.8.4"
	},
	"dependencies": {
		"@finsweet/ts-utils": "^0.37.3"
	}
}